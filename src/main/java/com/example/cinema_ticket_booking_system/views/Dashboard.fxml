<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.cinema_ticket_booking_system.controllers.DashboardController">
   <VBox prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
         AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <HBox prefHeight="50.0" prefWidth="800.0" style="-fx-background-color: #343a40;">
         <Button fx:id="homeButton" mnemonicParsing="false" onAction="#handleHomeButton" prefHeight="50.0"
                 prefWidth="100.0" style="-fx-background-color: transparent; -fx-text-fill: white;" text="Home"/>
         <Button fx:id="adminButton" mnemonicParsing="false" onAction="#handleAdminButton" prefHeight="50.0"
                 prefWidth="100.0" style="-fx-background-color: transparent; -fx-text-fill: white;" text="Admins"/>
      </HBox>
      <AnchorPane fx:id="contentPane" prefHeight="550.0" prefWidth="800.0" VBox.vgrow="ALWAYS"/>
   </VBox>
</AnchorPane>